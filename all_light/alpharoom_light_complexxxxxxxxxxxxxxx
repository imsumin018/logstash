for day in {1..31}
do
        if [ $day -lt 10 ] ;then
                file="cordoba-complex.2021-02-"0$day

        else
                file="cordoba-complex.2021-02-"$day
        fi
        echo `cat $file* | grep "알파룸" | grep Light >> ../output_all_alpharoom/all_light_202102_01_alpharoom_$file.json`
done
